		<![CDATA[SELECT
     NOTAS_DE_VENTA.`CODIGO` AS NOTAS_DE_VENTA_CODIGO,
     NOTAS_DE_VENTA.`FOLIO` AS NOTAS_DE_VENTA_FOLIO,
     NOTAS_DE_VENTA.`FECHA_DE_VENTA` AS NOTAS_DE_VENTA_FECHA_DE_VENTA,
