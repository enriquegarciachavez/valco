				<textFieldExpression class="java.lang.String"><![CDATA["DEL CLIENTE "+$P{clienteInicial}+" AL CLIENTE "+$P{clienteFinal}]]></textFieldExpression>
			</textField>
			<textField pattern="EEE, d MMM yyyy hh:mm aaa">
